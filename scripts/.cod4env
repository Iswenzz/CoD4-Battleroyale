#!/bin/bash
export NAME=sr_battleroyale
export GAME="/d/Program Files (x86)/Activision/Cod4Mod"
export MOD="$GAME/mods/$NAME"
export SR="$MOD/sr"

case "$(uname -s)" in
	Linux)
		export BIN_PREFIX=""
		export BIN_START="wine"
	;;
	CYGWIN*|MSYS*|MINGW*)
		export BIN_PREFIX=".exe"
		export BIN_START="start"
	;;
esac
