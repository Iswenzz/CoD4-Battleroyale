		#define PROGRESS_BAR_PROGRESS_STATDATA(pslot_s) \
		stat( int( tablelookup( dvarString(ui_table_name), 1, pslot_s, 3 ) ) )
		
		#define PROGRESS_BAR_WIDTH( pslot_s ) \
